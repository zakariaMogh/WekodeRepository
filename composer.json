{
    "name": "wekode/repository",
    "description": "Repository pattern setup",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "zakariaMogh",
            "email": "zakaria.dehaba@gmail.com"
        }
    ],
    "keywords": [
        "laravel",
        "repository"
    ],
    "minimum-stability": "dev",
    "require-dev": {
        "php": "^7.3|^8.0"
    },
    "autoload": {
        "psr-4": {
            "Wekode\\Repository\\": "/src"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Wekode\\Repository\\RepositorySetupServiceProvider"
            ]
        }
    },
    "scripts": {
        "post-install-cmd": [
            "php artisan vendor:publish --provider=\"Wekode\\Repository\\RepositorySetupServiceProvider\""
        ]
    }
}
